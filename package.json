{
  "name": "Kenote",
  "version": "1.0.0",
  "description": "Kenote for desktop",
  "main": "main.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "npm run compile:desktop && electron .",
    "clean": "rimraf dist",
    "dev": "better-npm-run dev",
    "compile": "npm run clean && better-npm-run compile",
    "compile:desktop": "npm run clean && better-npm-run compile:desktop",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:desktop": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "HISTORY": "desktop",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3028,
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "codecov": "^1.0.1",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "debug": "^2.5.2",
    "electron": "^1.4.15",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "lodash-webpack-plugin": "^0.10.6",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
